	<div class="container" style="margin-top: 50px">
	 <div class="row">
		 <div class="col-md-2">
		 </div>
		 <div class="col-md-8">
     <% @users.each do |other_user| %>
       <% if other_user != current_user %>
         <div class="col-md-3 col-sm-4 col-xs-6 ">
					 <%=link_to user_path(other_user) do %>
					 <%= image_tag other_user.image.url(:medium), class:"img-thumbnail" %>
					 <% end %>
					 <div>
					 	<div class="panel">
					 		<%= link_to other_user.first_name, user_path(other_user) %>

             <%= link_to(user_chats_path(current_user, :other_user => other_user.id), method: :post) do %>
						 	<%=   fa_icon "commenting" %>
						 <% end %>
					 	</div>
					 </div>

					 <br>
         </div>
       <% end %>
     <% end %>
	 </div>
		 <div class="col-md-2">
		 </div>
	 </div>
	</div>
